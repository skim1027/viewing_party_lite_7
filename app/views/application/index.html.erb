<% if current_user %>
  <%= link_to "Log Out", logout_path, method: :delete, data: {turbo_method: :delete}  %>
  <h3>Existing Users</h3>
  <% @users.each do |user| %>
    <p><%= user.email %></p>
  <% end %>
<% elsif current_user.nil? %>
  <%= button_to "Create a New User", register_path, method: :get %>
  <%= link_to "Log In", login_path %>
<% end %>

